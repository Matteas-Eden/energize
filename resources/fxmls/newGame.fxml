<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="newGamePane" maxHeight="900.0" maxWidth="1440.0" minHeight="768.0" minWidth="1024.0" prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.newGameController">
    <children>
        <VBox fx:id="newGameVBox" alignment="CENTER" prefHeight="768.0" prefWidth="1024.0" spacing="25">
            <children>
                <Label fx:id="newGameTitle" alignment="CENTER" contentDisplay="CENTER" prefHeight="40.0" text="New Game">
                    <font>
                        <Font size="60.0" />
                    </font>
                </Label>
                <Button fx:id="quickPlayButton" alignment="CENTER" mnemonicParsing="false" onAction="#quickPlayButtonPressed" prefHeight="40.0" prefWidth="200.0" text="Quick Play" textAlignment="CENTER" />
                <Button fx:id="customGameButton" alignment="CENTER" mnemonicParsing="false" onAction="#customGameButtonPressed" prefHeight="40.0" prefWidth="200.0" text="Custom Game" textAlignment="CENTER" />
                <Button fx:id="newGameBackButton" alignment="CENTER" mnemonicParsing="false" onAction="#newGameBackButtonPressed" prefHeight="40.0" prefWidth="200.0" text="Back to Main Menu" textAlignment="CENTER" />
            </children></VBox>
    </children>
</AnchorPane>
