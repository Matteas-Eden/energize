<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.canvas.Canvas?>
<AnchorPane fx:id="optionsPane" maxHeight="900.0" maxWidth="1440.0" minHeight="768.0" minWidth="1024.0" prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.optionsController">
    <children>
        <Canvas height="768.0" width="1024.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
        <VBox fx:id="optionsVBox" alignment="CENTER" prefHeight="768.0" prefWidth="1024.0" spacing="25">
            <children>
                <Label fx:id="Title" alignment="CENTER" contentDisplay="CENTER" prefHeight="40.0" text="Options">
                    <font>
                        <Font size="60.0" />
                    </font>
                </Label>
                <Button fx:id="changeResolutionButton" alignment="CENTER" mnemonicParsing="false" onAction="#changeResolutionButtonPressed" prefHeight="40.0" prefWidth="200.0" text="Change Resolution" textAlignment="CENTER" />
                <Button fx:id="changeVolumeButton" alignment="CENTER" mnemonicParsing="false" onAction="#changeVolumeButtonPressed" prefHeight="40.0" prefWidth="200.0" text="Change Volume" textAlignment="CENTER" />
                <Button fx:id="editKeyBindsButton" alignment="CENTER" mnemonicParsing="false" onAction="#editKeyBindsButtonPressed" prefHeight="40.0" prefWidth="200.0" text="Edit Key Binds" textAlignment="CENTER" />
                <Button fx:id="selectDifficultyButton" alignment="CENTER" mnemonicParsing="false" onAction="#selectDifficultyButtonPressed" prefHeight="40.0" prefWidth="200.0" text="Select Difficulty" textAlignment="CENTER" />
                <Button fx:id="optionsBackButton" alignment="CENTER" mnemonicParsing="false" onAction="#optionsBackButtonPressed" prefHeight="40.0" prefWidth="200.0" text="Back to Main Menu" textAlignment="CENTER" />
            </children></VBox>
    </children>
</AnchorPane>
