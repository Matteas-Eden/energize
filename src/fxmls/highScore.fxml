<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.canvas.Canvas?>
<AnchorPane fx:id="highScorePane" maxHeight="900.0" maxWidth="1440.0" minHeight="768.0" minWidth="1024.0" prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.highScoreController">
    <children>
        <Canvas height="768.0" width="1024.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
        <VBox fx:id="highScoreVBox" alignment="CENTER" prefHeight="768.0" prefWidth="1024.0" spacing="25">
            <children>
                <Label fx:id="highScoreTitle" alignment="CENTER" contentDisplay="CENTER" prefHeight="40.0" text="High Score">
                    <font>
                        <Font size="60.0" />
                    </font>
                </Label>
                <Button fx:id="highScoreButton" alignment="CENTER" mnemonicParsing="false" onAction="#highScoreButtonPressed" prefHeight="40.0" prefWidth="200.0" text="View High Scores" textAlignment="CENTER" />
                <Button fx:id="highScoreBackButton" alignment="CENTER" mnemonicParsing="false" onAction="#highScoreBackButtonPressed" prefHeight="40.0" prefWidth="200.0" text="Back to Main Menu" textAlignment="CENTER" />
            </children></VBox>
    </children>
</AnchorPane>
